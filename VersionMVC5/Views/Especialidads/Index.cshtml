@model IEnumerable<VersionMVC5.Models.Especialidad>

@{
    ViewBag.Title = "Index";
}

<h2>Especialidad</h2>

<p>
    @Html.ActionLink("Ingresar nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nom_Especialidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nom_Especialidad)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id_Especialidad }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id_Especialidad }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id_Especialidad })
        </td>
    </tr>
}

</table>
