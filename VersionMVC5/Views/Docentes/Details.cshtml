@model VersionMVC5.Models.Docente

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Docente</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nom_Docente)
        </dd>

        <dt>
            Direccion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Dire_Docente)
        </dd>

        <dt>
           Telefono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tel_Docente)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Docente }) |
    @Html.ActionLink("Regresar", "Index")
</p>
